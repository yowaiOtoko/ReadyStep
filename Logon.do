	function slideMainDiv() {
		if (!$("#errMsg").is(":visible")) {
			var topPos = $(".cbs-main_container-logon_screen").css("top")
			topPos = (parseInt(topPos.substring(0, topPos.length - 2)) + 24) + "px";
			$(".cbs-main_container-logon_screen").animate({top:topPos});
			// var leftContPos = $(".cbs-left-container").css("top")
			// leftContPos = (parseInt(leftContPos.substring(0, leftContPos.length - 2)) + 24) + "px";
			// $(".cbs-left-container").animate({top:leftContPos});
		}
	}

	function rememberClick() {
		
		if (document.forms[0].systemLoginName.value == "") {
			slideMainDiv();
			$("#errMsg").text(EMPTY_LOGIN_NAME);
			$("#errMsg").slideDown();
			return false;
		} else if (document.forms[0].systemPassword.value == "") {
			slideMainDiv();
			$("#errMsg").text(EMPTY_PASSWORD);
			$("#errMsg").slideDown();
			return false;
		}
		
		
		
		return true;
	}

	function login() {
		if (rememberClick()) {
			document.forms[0].submit();
		}
	}

	function initForm() {
		
		
		
		let name = document.forms[0].systemLoginName.value;
		if (name != null && name != "") {
			document.forms[0].systemPassword.focus();
		} else {
			document.forms[0].systemLoginName.focus();
		}
		
	}

	function loginViaEnter(evt) {
		evt = (evt) ? evt : event;
		var target = (evt.target) ? evt.target : evt.srcElement;
		var form = target.form;
		var charCode = (evt.charCode) ? evt.charCode :
			((evt.which) ? evt.which : evt.keyCode);
		if (charCode == 13) {
			login();
			return false;
		}
		return true;
	}
