<script type="text/javascript">
$(function() {
	var pull           = $('.cbs-menu-btn>a');
	var menu           = $('.cbs-left-container');
	var rightContainer = $('.cbs-right-container');
	var menuHeight     = menu.height();

	$(pull).on('click', function(e) {
		e.preventDefault();
		menu.slideToggle();
		if ("0px" == rightContainer.css("left")) {
			rightContainer.css( "left", "200px" );
		} else {
			rightContainer.css( "left", "0px" );
		}
	});

	$(window).resize(function() {
		if ($(".cbs-left-container").css("display") == "block") {
			rightContainer.css( "left", "200px" );
			if (menu.is(':hidden')) {
				menu.removeAttr('style');
			}
		}
		if (menu.is(':hidden')) {
			rightContainer.css("left", "0px");
		}
		resizeButtonTable();
	});

	resizeButtonTable();
	
	
	var url = "/cbs/logon.jsp";
	
	if (url.startsWith("/cbs/index.jsp") || url.startsWith("/cbs/indexUser.jsp") || url.startsWith("/cbs/indexReseller.jsp")) {
		$("#langMenu").show();
		if ($(window).width() > 800) {
			$("#loginName").show();
		} else {
			$("#loginName").hide();
		}
		$("#logoutBtn").show();
		$("#liLang").show();
		$("#liLogout").show();
	} else {
		$("#loginName").hide();
		$("#logoutBtn").hide();
		$("#liLogout").hide();
		
		
		
		if (url.startsWith("/cbs/logon.jsp")) {
			$("#langMenu").show();
			$("#liLang").show();
		
		
		
		} else if (url.indexOf("downloadReport.jsp") != -1) {
			$("#langMenu").hide();
			$("#liLang").hide();
			$("#liDownload").hide();
			$(".cbs-new-dl_btn").hide();
		} else {
			$(".cbs-mobile_view").hide();
			$("#langMenu").hide();
			$("#liLang").hide();
		}
	}
	if ($("#langMenu ul li").length == 1) {
		$("#langMenu ul").css("width", "240");
	}
});
</script>

